{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#00ffcc",
          "background": "#44475a",
          "style": "plain",
          "template": "\u250f[\u2388 <#ffffff>\ueb99</> {{ .UserName }} <#ff79c6>@</> <#ffffff>\uf108</> {{ .HostName }}]",
          "type": "session"
        },
        {
          "foreground": "#f1fa8c",
          "background": "#44475a",
          "style": "plain",
          "template": "[‚è≥ <#ffffff>{{ .FormattedMs }}s</>]",
          "type": "executiontime"
        },
        {
          "foreground": "#50fa7b",
          "background": "#44475a",
          "style": "plain",
          "template": "[üíª <#ffffff>{{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB RAM</>]",
          "type": "sysinfo"
        },
        {
          "foreground": "#ff79c6",
          "background": "#44475a",
          "properties": {
            "time_format": "Monday at 3:04:05 PM"
          },
          "style": "plain",
          "template": "[üìÖ <#ffffff>{{ .CurrentDate | date .Format }}</>]",
          "type": "time"
        },
        {
          "foreground": "#bd93f9",
          "background": "#44475a",
          "style": "plain",
          "template": "[\uf292]",
          "type": "root"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#8be9fd",
          "background": "#282a36",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": "\u2516[\uf07c <#ffffff>{{ .Path }}</>]",
          "type": "path"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#bd93f9",
          "background": "#282a36",
          "style": "plain",
          "template": "\u2514\u2500üîª",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
