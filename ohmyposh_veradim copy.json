{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#7eb8da",
          "background": "#2e3440",
          "style": "powerline",
          "template": "\u250f[<#ffffff>\ueb99</> {{ .UserName }}@<#ffffff>\uf108</> {{ .HostName }}]",
          "type": "session"
        },
        {
          "foreground": "#ffffff",
          "background": "#a3be8c",
          "style": "powerline",
          "template": "[\uf017 {{ .CurrentDate | date \"15:04:05\" }}]",
          "type": "time"
        },
        {
          "foreground": "#ffffff",
          "background": "#d08770",
          "style": "powerline",
          "template": "[\uf252 {{ .FormattedMs }}s]",
          "type": "executiontime"
        },
        {
          "foreground": "#ffffff",
          "background": "#b48ead",
          "style": "powerline",
          "template": "[RAM: {{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB]",
          "type": "sysinfo"
        },
        {
          "foreground": "#ffffff",
          "background": "#5e81ac",
          "style": "powerline",
          "template": "{{ if eq .UpstreamIcon \"\" }}\uf126{{ else }}{{ .UpstreamIcon }}{{ end }} {{ .HEAD }}{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} | {{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }}",
          "type": "git"
        },
        {
          "foreground": "#ffffff",
          "background": "#88c0d0",
          "style": "powerline",
          "template": "[AWS: {{ .Profile }}{{if .Region}} @{{ .Region }}{{ end }}]",
          "type": "aws"
        },
        {
          "foreground": "#ffffff",
          "background": "#bf616a",
          "style": "powerline",
          "template": "[Kube: {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}]",
          "type": "kubectl"
        },
        {
          "foreground": "#bf616a",
          "style": "plain",
          "template": "\uf0e7",
          "type": "root"
        },
        {
          "foreground": "#ffffff",
          "background": "#d08770",
          "style": "powerline",
          "template": "[\uea6c Error, check your command]",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#98bfad",
          "background": "#4c566a",
          "style": "powerline",
          "template": "\u2516[{{ .Path }}]",
          "type": "path"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#7eb8da",
          "style": "plain",
          "template": "\u2514\u2500Î”",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
